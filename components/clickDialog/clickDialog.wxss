.wx-mask {
    position: fixed;
    z-index: 4001;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0,0,0,0.8);
}

.wx-dialog {
    position: fixed;
    z-index: 5000;
    width: 600rpx;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    text-align: center;
    border-radius: 3px;
    overflow: hidden;
    background: linear-gradient(135deg,transparent 7px,rgb(12,99,82) 0) top left,linear-gradient(-135deg,transparent 7px,rgb(12,99,82) 0) top right,linear-gradient(-45deg,transparent 7px,rgb(12,99,82) 0) bottom right,linear-gradient(45deg,transparent 7px,rgb(12,99,82) 0) bottom left;
    background-size: 51% 51%;
    background-repeat: no-repeat;
}

.wx-dialog-title {
    font-size: 18px;
    padding: 15px;
    color: #fff;
    border-bottom: 2rpx solid #78cfbc;
    margin-bottom: 20rpx;
}

.wx-dialog-footer {
    display: flex;
    align-items: center;
    line-height: 45px;
    font-size: 17px;
    padding-top: 30rpx;
}

.wx-dialog-position {
    position: relative;
}

.weui-cell {
    padding: 10px 15px;
    position: relative;
    border-bottom: 1px solid #D5D5D6;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

icon {
    position: absolute;
    top: 10px;
    left: 15px;
}

.weui-cell__bd {
    text-align: left;
    margin-left: 40px;
    -webkit-box-flex: 1;
    flex: 1;
}

.title {
    color: #353535;
    font-size: 30rpx;
}

.contentInfo {
    color: #3CC51F;
    font-size: 28rpx;
}

.change_line {
    display: inline;
}

.dian1 {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 15rpx;
    left: 15rpx;
}

.dian2 {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 15rpx;
    right: 15rpx;
    transform: rotate(90deg);
}

.dian3 {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    bottom: 15rpx;
    right: 15rpx;
}

.dian4 {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    bottom: 15rpx;
    left: 15rpx;
    transform: rotate(90deg);
}

.wx-dialog-btn {
    position: relative;
    height: 130rpx;
    width: 400rpx;
    display: inline-block;
    text-align: center;
    left: 50%;
    transform: translate(-50%,0);
}

.wx-dialog-btn text {
    color: #fff;
    font-size: 30rpx;
    position: absolute;
    z-index: 100;
    line-height: 55rpx;
    top: 19rpx;
    letter-spacing: 5rpx;
    left: 50%;
    transform: translate(-50%,0);
}

.button {
    width: 339rpx;
    height: 90rpx;
    position: relative;
}
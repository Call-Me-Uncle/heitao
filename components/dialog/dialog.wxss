.wx-mask {
    position: fixed;
    z-index: 1000;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0,0,0,0.5);
}

.wx-mask1 {
    position: fixed;
    z-index: 1000;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0,0,0,0.1);
}

.wx-dialog {
    position: fixed;
    z-index: 4000;
    width: 80%;
    max-width: 600rpx;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    text-align: center;
    border-radius: 3px;
    overflow: hidden;
    background: linear-gradient(135deg,transparent 7px,rgb(12,99,82) 0) top left,linear-gradient(-135deg,transparent 7px,rgb(12,99,82) 0) top right,linear-gradient(-45deg,transparent 7px,rgb(12,99,82) 0) bottom right,linear-gradient(45deg,transparent 7px,rgb(12,99,82) 0) bottom left;
    background-size: 51% 51%;
    background-repeat: no-repeat;
}

.wx-dialog1 {
    position: fixed;
    z-index: 4000;
    width: 80%;
    max-width: 600rpx;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    text-align: center;
    border-radius: 3px;
    overflow: hidden;
    background: linear-gradient(135deg,transparent 7px,#003b35 0) top left,linear-gradient(-135deg,transparent 7px,#003b35 0) top right,linear-gradient(-45deg,transparent 7px,#003b35 0) bottom right,linear-gradient(45deg,transparent 7px,#003b35 0) bottom left;
    background-size: 51% 51%;
    background-repeat: no-repeat;
}

.wx-dialog-title0 {
    padding: 15px;
}

.wx-dialog-title1 {
    font-size: 18px;
    padding: 15px;
    color: #fff;
    border-bottom: 2rpx solid #78cfbc;
}

.wx-dialog-content {
    width: 545rpx;
    padding: 15px;
    min-height: 40px;
    max-height: 300px;
    font-size: 16px;
    line-height: 1.3;
    word-wrap: break-word;
    word-break: break-all;
    overflow: hidden;
    text-align: left;
}

.wx-dialog-footer {
    display: flex;
    align-items: center;
    line-height: 45px;
    font-size: 17px;
    justify-content: space-around;
}

.wx-dialog-position {
    position: relative;
}

.wx-dialog-btn1 {
    position: relative;
    display: inline-block;
    text-align: center;
}

.wx-dialog-btn1 text {
    color: #fff;
    font-size: 28rpx;
    position: absolute;
    z-index: 100;
    line-height: 55rpx;
    top: 19rpx;
    letter-spacing: 5rpx;
    left: 50%;
    transform: translate(-50%,0);
}

.wx-dialog-btn1 .button {
    width: 340rpx;
    height: 90rpx;
    position: relative;
}

.wx-dialog-btn2 {
    text-align: center;
    position: relative;
}

.wx-dialog-btn2 text {
    color: #fff;
    font-size: 30rpx;
    position: absolute;
    z-index: 100;
    line-height: 27rpx;
    top: 19rpx;
    letter-spacing: 5rpx;
    left: 50%;
    transform: translate(-50%,0);
}

.wx-dialog-btn2 .button {
    width: 238rpx;
    height: 63rpx;
    position: relative;
}

